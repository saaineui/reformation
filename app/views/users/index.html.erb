<h1>Users</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Token</th>
            <th>Token Confirmation</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        <% @users.each do |user| %>
            <%= render user %>
        <% end %>
    </tbody>
</table>